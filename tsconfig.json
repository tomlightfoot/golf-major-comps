{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "module": "esnext",
    "target": "es5",
    "lib": ["dom", "es2015"],
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}